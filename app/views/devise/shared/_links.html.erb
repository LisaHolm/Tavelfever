<style media="screen">



</style>

<%- if controller_name != 'sessions' %>
  <%= link_to "Log in", new_session_path(resource_name), class: "waves-effect waves-light btn-large purple darken-4" %>
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to omniauth_authorize_path(resource_name, provider), class: "waves-effect waves-light btn-large purple darken-4" do %>
      Use  <%= OmniAuth::Utils.camelize(provider) %> <i class="fab fa-facebook-square fa-2x"></i> <br />
    <% end %>
    <br>
  <% end -%>
<% end -%>

<br>
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Create Account", new_registration_path(resource_name), class:"purple-text darken-4" %><br />
  <br>
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <%= link_to "Forgot your password?", new_password_path(resource_name), class: "purple-text darken-4" %><br />
  <br>
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <%= link_to "Didn't receive confirmation instructions?", new_confirmation_path(resource_name), class: "purple-text darken-4" %><br />
  <br>
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <%= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name), class: "purple-text darken-4" %><br />
  <br>
<% end -%>
